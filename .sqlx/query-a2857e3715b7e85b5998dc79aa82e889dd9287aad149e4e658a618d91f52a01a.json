  "query": "\n        UPDATE users\n        SET name = COALESCE($1, name),\n            email = COALESCE($2, email),\n            password = COALESCE($3, password),\n            updated_at = CURRENT_TIMESTAMP\n        WHERE id = $4\n        RETURNING id, name, email, password, \n                  created_at::TEXT as \"created_at!\", \n                  updated_at::TEXT as \"updated_at!\"\n        ",
